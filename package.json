    "react-dom": "^19",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@hono/zod-validator": "^0.7.6",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
